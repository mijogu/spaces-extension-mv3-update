{
   "background": {
      "service_worker": "js/tests/service-worker-test.js",
      "type": "module"
   },
   "action": {
      "default_icon": "img/icon128.png",
      "default_popup": "popup.html",
      "default_title": "Spaces Test"
   },
   "commands": {
      "spaces-move": {
         "description": "Move active tab",
         "suggested_key": {
            "default": "Ctrl+Shift+M",
            "mac": "Command+Shift+M"
         }
      },
      "spaces-switch": {
         "description": "Switch between spaces",
         "suggested_key": {
            "default": "Ctrl+Shift+S",
            "mac": "Command+Shift+S"
         }
      }
   },
   "description": "Intuitive tab management - Test Version",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   },
   "incognito": "split",
   "key": "",
   "manifest_version": 3,
   "minimum_chrome_version": "88",
   "name": "Spaces MV3 Test",
   "options_page": "spaces.html",
   "permissions": [ "tabs", "storage", "history", "unlimitedStorage", "contextMenus" ],
   "host_permissions": [ "http://*/*", "https://*/*" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.3-test",
   "web_accessible_resources": [
        {
            "resources": ["popup-test.html", "popup-test-new-sw.html"],
            "matches": ["<all_urls>"]
        }
    ]
} 